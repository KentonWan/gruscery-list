    "dotenv": "^5.0.0",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "sequelize": "^4.32.6"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "jasmine": "^3.3.0",
    "request": "^2.88.0"
